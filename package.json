{
  "name": "@atmus/nestjs-openai",
  "version": "1.0.0",
  "private": false,
  "description": "NestJS - OpenAI integration",
  "keywords": [
    "openai",
    "nestjs",
    "nodejs"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "deploy": "sh ./publish.sh",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "release": "standard-version",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atmus-tecnologia/nestjs-openai.git"
  },
  "author": "Felipe Medeiros <medeiros.dev@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atmus-tecnologia/nestjs-openai/issues"
  },
  "homepage": "https://github.com/atmus-tecnologia/nestjs-openai#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "openai": "^4.24.7"
  }
}
